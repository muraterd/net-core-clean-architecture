@addTagHelper *, WebCMS

<aside class="left-sidebar bg-sidebar">
    <div id="sidebar" class="sidebar sidebar-with-footer">
        <!-- Aplication Brand -->
        <div class="app-brand">
            <a href="/admin">
                <img class="brand-icon" src="~/admin/public/img/sleek-icon.png" alt="" height="33" />
                <span class="brand-name">İstanbay</span>
            </a>
        </div>

        <!-- begin sidebar scrollbar -->
        <div class="sidebar-scrollbar">

            <!-- sidebar menu -->
            <ul class="nav sidebar-inner" id="sidebar-menu">
                <menu-item title="Dashboard" url="/admin" icon-class-name="mdi mdi-view-dashboard-outline" exact="true" />
                <menu-item title="Kullanıcılar" url="/admin/users" icon-class-name="mdi mdi-account-multiple" />
                <menu-item title="Sayfalar" url="/admin/pages" icon-class-name="mdi mdi-file-document-box-multiple" />
            </ul>

        </div>
    </div>
</aside>