@addTagHelper *, WebCMS
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<!DOCTYPE html>
<html lang="en" dir="ltr">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />

    <title>Sleek - Admin Dashboard Template</title>

    <!-- GOOGLE FONTS -->
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,500|Poppins:400,500,600,700|Roboto:400,500" rel="stylesheet" />
    <link href="https://cdn.materialdesignicons.com/3.0.39/css/materialdesignicons.min.css" rel="stylesheet" />

    <!-- PLUGINS CSS STYLE -->
    <link href="~/admin/public/plugins/toaster/toastr.min.css" rel="stylesheet" />
    <link href="~/admin/public/plugins/nprogress/nprogress.css" rel="stylesheet" />
    <link href="~/admin/public/plugins/flag-icons/css/flag-icon.min.css" rel="stylesheet" />
    <link href="~/admin/public/plugins/jvectormap/jquery-jvectormap-2.0.3.css" rel="stylesheet" />
    <link href="~/admin/public/plugins/ladda/ladda.min.css" rel="stylesheet" />
    <link href="~/admin/public/plugins/select2/css/select2.min.css" rel="stylesheet" />
    <link href="~/admin/public/plugins/daterangepicker/daterangepicker.css" rel="stylesheet" />

    <!-- SLEEK CSS -->
    <link id="sleek-css" rel="stylesheet" href="~/admin/public/css/sleek.css" />

    <!-- Main CSS -->
    <link id="sleek-css" rel="stylesheet" href="~/admin/public/css/main.css" />

    <!-- FAVICON -->
    <link href="~/admin/public/img/favicon.png" rel="shortcut icon" />

    <!--
      HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries
    -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <script src="~/admin/public/plugins/jquery/jquery.min.js"></script>
    <script src="~/admin/public/plugins/nprogress/nprogress.js"></script>

    <script src="~/admin/public/plugins/tinymce/tinymce.min.js"></script>
    <script>tinymce.init({ selector: 'textarea', height: 500 });</script>
</head>


<body class="sidebar-fixed sidebar-dark header-light header-fixed" id="body">
    <script>
        NProgress.configure({ showSpinner: false });
        NProgress.start();</script>

    <div class="mobile-sticky-body-overlay"></div>

    <div class="wrapper">

        <vc:left-menu />

        <div class="page-wrapper">
            <partial name="~/Areas/Admin/Views/Partials/MainHeader.cshtml" />

            @RenderBody()

            <footer class="footer mt-auto">
                <div class="copyright bg-white">
                    <p>
                        &copy; <span id="copy-year">2019</span> Copyright Sleek Dashboard Bootstrap Template by
                        <a class="text-primary"
                           href="http://www.iamabdus.com/"
                           target="_blank">Abdus</a>.
                    </p>
                </div>
                <script>
                    var d = new Date();
                    var year = d.getFullYear();
                    document.getElementById("copy-year").innerHTML = year;</script>
            </footer>

        </div>
    </div>


    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDCn8TFXGg17HAUcNpkwtxxyT9Io9B_NcM" defer></script>
    <script src="~/admin/public/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/admin/public/plugins/toaster/toastr.min.js"></script>
    <script src="~/admin/public/plugins/slimscrollbar/jquery.slimscroll.min.js"></script>
    <script src="~/admin/public/plugins/charts/Chart.min.js"></script>
    <script src="~/admin/public/plugins/ladda/spin.min.js"></script>
    <script src="~/admin/public/plugins/ladda/ladda.min.js"></script>
    <script src="~/admin/public/plugins/jquery-mask-input/jquery.mask.min.js"></script>
    <script src="~/admin/public/plugins/select2/js/select2.min.js"></script>
    <script src="~/admin/public/plugins/jvectormap/jquery-jvectormap-2.0.3.min.js"></script>
    <script src="~/admin/public/plugins/jvectormap/jquery-jvectormap-world-mill.js"></script>
    <script src="~/admin/public/plugins/daterangepicker/moment.min.js"></script>
    <script src="~/admin/public/plugins/daterangepicker/daterangepicker.js"></script>
    <script src="~/admin/public/plugins/jekyll-search.min.js"></script>
    <script src="~/admin/public/js/sleek.js"></script>
    <script src="~/admin/public/js/chart.js"></script>
    <script src="~/admin/public/js/date-range.js"></script>
    <script src="~/admin/public/js/map.js"></script>
    <script src="~/admin/public/js/custom.js"></script>

</body>
</html>
